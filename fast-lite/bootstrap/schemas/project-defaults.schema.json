{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Project Defaults",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "defaults": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "billing_account": {
          "type": "string"
        },
        "prefix": {
          "type": "string"
        },
        "services": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^[a-z-]+\\.googleapis\\.com$"
          }
        }
      }
    },
    "overrides": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "billing_account": {
          "type": "string"
        },
        "prefix": {
          "type": "string"
        },
        "services": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^[a-z-]+\\.googleapis\\.com$"
          }
        }
      }
    }
  }
}